    <Style TargetType="{x:Type Path}" x:Key="1">
        <Setter Property="Fill">
            <Setter.Value>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FF97FF25"/>
                    <GradientStop Color="#FF0B1F14" Offset="1"/>
                    <GradientStop Color="#FF4EDD3E" Offset="0.309"/>
                    <GradientStop Color="#FF286F43" Offset="0.723"/>
                    <GradientStop Color="#FF248C34" Offset="0.516"/>
                    <GradientStop Color="#FF132F1E" Offset="0.883"/>
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
        <Setter Property="Data">
            <Setter.Value>
            <GeometryGroup>
                <PathGeometry>
                    <PathGeometry.Figures>
                        <PathFigureCollection>
                            <PathFigure IsClosed="true" StartPoint="50,50">
                                <PathFigure.Segments>
                                    <PathSegmentCollection>
                                        <BezierSegment Point1="75,300" Point2="125,100" Point3="150,50"/>
                                        <BezierSegment Point1="125,300" Point2="75,100"  Point3="50,50"/>
                                    </PathSegmentCollection>
                                </PathFigure.Segments>
                            </PathFigure>
                        </PathFigureCollection>
                    </PathGeometry.Figures>
                </PathGeometry>
            </GeometryGroup>
                </Setter.Value>
        </Setter>
    </Style>

